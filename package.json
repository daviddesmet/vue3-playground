{
  "name": "vue3-playground",
  "version": "0.0.1",
  "private": true,
  "description": "Vue 3 + Vite Playground",
  "author": "David De Smet <david.desmet@definityfirst.com>",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "start": "yarn dev & wait-on tcp:3000 -v",
    "prepare": "husky install",
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.ts,.vue .",
    "lint:all:stylelint": "yarn lint:stylelint \"src/**/*.{vue,scss}\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,ts,vue,html,md}\"",
    "lint:ci:eslint": "yarn eslint --quiet --ext .js,.ts,.vue .",
    "lint:ci:stylelint": "yarn stylelint --quiet \"src/**/*.{vue,scss}\"",
    "lint:ci:prettier": "yarn prettier --check --loglevel error \"**/*.{js,json,css,scss,ts,vue,html,md}\"",
    "lint:ci": "run-s lint:ci:*",
    "lint": "run-s lint:all:*",
    "test:unit": "cross-env VUE_APP_TEST=unit vite-jest",
    "test:unit:file": "yarn test:unit --bail --findRelatedTests",
    "test:unit:help": "yarn test:unit help",
    "test:unit:watch": "yarn test:unit --watch --notify --notifyMode change",
    "test:unit:ci": "yarn test:unit --coverage --ci --reporters=default --reporters=jest-junit",
    "test:e2e": "cross-env VUE_APP_TEST=e2e cypress open-ct",
    "test:e2e:spec": "cross-env VUE_APP_TEST=e2e cypress run-ct --quiet --reporter spec",
    "test:e2e:ci": "cross-env VUE_APP_TEST=e2e cypress run-ct --quiet --reporter mocha-junit-reporter",
    "test": "run-s test:unit test:e2e",
    "test:ci": "run-s test:unit:ci test:e2e:ci"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "vue": "^3.1.5",
    "vue-class-component": "^8.0.0-rc.1",
    "vue-decorator": "^1.1.3",
    "vue-router": "4.0.10",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.0.4",
    "@cypress/vue": "^3.0.3",
    "@headlessui/vue": "^1.4.0",
    "@heroicons/vue": "^1.0.3",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/line-clamp": "^0.2.1",
    "@tailwindcss/typography": "^0.4.1",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.1.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.12",
    "@vuedx/typecheck": "~0.7.4",
    "@vuedx/typescript-plugin-vue": "~0.7.4",
    "autoprefixer": "^10.3.1",
    "cross-env": "^7.0.3",
    "cypress": "^8.1.0",
    "cypress-multi-reporters": "^1.5.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.15.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-environment-jsdom": "^27.0.6",
    "jest-junit": "^12.2.0",
    "lint-staged": "^12.1.2",
    "mocha": "^9.0.3",
    "mocha-junit-reporter": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "sass": "^1.37.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.3.5",
    "vite": "^2.4.4",
    "vite-jest": "^0.0.3",
    "vite-tsconfig-paths": "^3.3.13",
    "vue-eslint-parser": "^7.10.0",
    "vue-property-decorator": "^10.0.0-rc.3",
    "wait-on": "^6.0.0"
  },
  "gitHooks": {
    "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
  }
}
